{
  "name": "react-universally",
  "version": "9.0.0",
  "description": "A starter kit giving you the minimum requirements for a production ready universal react application.",
  "main": "build/server/main.js",
  "engines": {
    "node": "~6.9.1"
  },
  "scripts": {
    "clean": "node tools/scripts/clean",
    "development": "node tools/development",
    "build": "node tools/scripts/build",
    "analyze": "node tools/scripts/analyze",
    "start": "node build/server",
    "deploy": "node tools/scripts/deploy",
    "lint": "eslint src",
    "flow": "node tools/scripts/flow",
    "flow:defs": "flow-typed install -f 0.34 --overwrite",
    "flow:report": "flow-coverage-report -i 'src/**/*.js' -t html -t json -t text",
    "flow:remove": "node tools/scripts/removeTypes.js && rimraf tools/scripts/removeTypes.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ctrlplusb/react-universally.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "starter kit",
    "universal",
    "javascript",
    "express",
    "webpack"
  ],
  "author": "Sean Matheson <sean@ctrlplusb.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ctrlplusb/react-universally/issues"
  },
  "homepage": "https://github.com/ctrlplusb/react-universally#readme",
  "dependencies": {
    "app-root-dir": "^1.0.2",
    "code-split-component": "^2.0.0-alpha.2",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "helmet": "^3.1.0",
    "hpp": "^0.2.1",
    "normalize.css": "^5.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-helmet": "^3.2.2",
    "react-router": "^4.0.0-alpha.5",
    "serialize-javascript": "^1.3.0",
    "source-map-support": "^0.4.6",
    "uuid": "^3.0.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-es2015-destructuring": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "chokidar": "^1.6.1",
    "colors": "^1.1.2",
    "css-loader": "^0.26.0",
    "dotenv": "^2.0.0",
    "eslint": "^3.11.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-flowtype": "^2.28.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.36.0",
    "flow-coverage-report": "^0.2.0",
    "flow-remove-types": "^1.0.5",
    "flow-typed": "^2.0.0",
    "glob": "^7.1.1",
    "happypack": "^3.0.0",
    "json-loader": "^0.5.4",
    "match-require": "^2.0.0",
    "md5": "^2.2.1",
    "node-notifier": "^4.6.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "regenerator-runtime": "^0.10.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "sw-precache-webpack-plugin": "^0.6.3",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.25",
    "webpack-bundle-analyzer": "^2.1.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-md5-hash": "^0.0.5",
    "webpack-node-externals": "^1.5.4"
  }
}
