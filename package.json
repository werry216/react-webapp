{
  "name": "react-universally",
  "version": "0.0.0",
  "description": "A mildly opinionated ultra low dependency universal react boilerplate.",
  "main": "build/server/main.js",
  "scripts": {
    "clean": "rimraf build",
    "development": "npm run clean && cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development node ./devServer",
    "build": "npm run clean && webpack --env.mode production --config webpack.client.config.js && webpack --env.mode production --config webpack.server.config.js",
    "start": "node build/server/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ctrlplusb/react-universally.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "universal",
    "javascript",
    "express",
    "webpack"
  ],
  "author": "Sean Matheson <sean@ctrlplusb.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ctrlplusb/react-universally/issues"
  },
  "homepage": "https://github.com/ctrlplusb/react-universally#readme",
  "dependencies": {
    "compression": "1.6.2",
    "express": "^4.13.4",
    "helmet": "2.1.1",
    "history": "3.0.0",
    "hpp": "0.2.1",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "react-router": "2.4.1",
    "serialize-javascript": "1.3.0",
    "source-map-support": "0.4.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.4.0",
    "babel-cli": "6.10.1",
    "babel-core": "6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015-webpack": "6.4.1",
    "babel-preset-react": "^6.5.0",
    "chokidar": "1.5.2",
    "cross-env": "1.0.8",
    "dotenv": "^2.0.0",
    "json-loader": "0.5.4",
    "node-notifier": "4.6.0",
    "path": "^0.12.7",
    "react-hot-loader": "3.0.0-beta.2",
    "rimraf": "^2.5.2",
    "standard": "7.1.2",
    "webpack": "2.1.0-beta.13",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-node-externals": "1.2.0",
    "webpack-validator": "2.1.3"
  }
}
